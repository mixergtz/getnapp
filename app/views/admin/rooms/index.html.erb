<div class="container">
  <h3>Todas las rooms</h3>

  <table class="table table-hover">
    <thead>
        <tr>
            <th>Name</th>
            <th>Address</th>
            <th>Options</th>
        </tr>
    </thead>
    <tbody>
        <% @rooms.each do |room| %>
          <tr>
            <th><%= room.name %></th>
            <th><%= room.address %></th>
            <th><%= link_to "See Room", admin_room_path(room) %></th>
          </tr>
        <% end %>
    </tbody>
  </table>

  <%= link_to "New room", new_admin_room_path %>
</div>

